    <style>
      body {

font-family: sans-serif !important;
}

ul li {
position: relative;
}

ul li a::before {
content: "";
position: absolute;
bottom: 0px;
left: 0;
width: 0px;
height: 4px;
transition: 0.3s;
background-color: #fff !important;

}

ul li a::after {
content: "";
position: absolute;
top: 0px;
right: 0;
width: 0px;
height: 4px;
transition: 0.5s ease;
background-color: #fff !important;

}

ul li:hover a::before,
ul li:hover a::after {
content: "";
width: 100%;
}
.card {
            border-radius: 5px;
            padding: 20px;
            background-color: #ebecf0;
            border-radius: 20px;
            box-shadow: 2px 2px 5px #bfc5db, -2px -2px 10px #fff !important;
        }
        .card:hover {
            box-shadow: inset 2px 2px 5px #babecc, inset -2px -2px 10px #fff !important;
        }
        .btn2{
          background-color: #fa2d2d ;
          position: relative;
        }
        .btn2::before{
          content: "";
           position: absolute;
           bottom: -7px;
           left: 3px;
           height: 7px;
           width: 100%;
           background-color: #b61408;
           transform:rotate(0deg) skewX(45deg) ;
        }
        .btn2::after{
          content: "";
           position: absolute;
           top: 4px;
           right: -7px;
           height: 100%;
           width: 7px;
           background-color: #b61408;
           transform:rotate(0deg) skewY(45deg) ;
        }
        .btn2:hover{
          background-color: #eee;
          color: #111;
        }
    .btn2:hover::before {
      background-color: #babecc;
    }
    .btn2:hover::after{
      background-color: #babecc;
    }
    span{
        color: black;
    }
    </style>
</head>

<body>

