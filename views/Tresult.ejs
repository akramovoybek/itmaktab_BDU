  <div class="container">
 
      <table class="striped">
        <thead>
          <tr>
            <th>N<sub>0</sub></th>
            <th>full name</th>
            <th>rating</th>
            <th>at protsent</th>
            <th>Date</th>
          </tr>
        </thead>

        <tbody id="place">
          <% var k=1 %>
          <% for(result of results){ %>
          <tr>
            <td><%= k %></td><% k++ %>
            <td><%= result.studentId.firstname %> <%= result.studentId.lastname %></td>
            <td><%= result.nechta %>/<%= result.counttest %>:</td>
            <td><%= (result.nechta*100/result.counttest).toPrecision(3) %>%</td>
            <td><%= result.addDate %></td>
          </tr>
         <% } %>
         

        </tbody>
      </table>

  
