    ul li {
      position: relative;
    }
    ul li a::before{
      content: "";
      position: absolute;
     bottom: 0px;
     left: 0;
      width: 0px;
      height: 4px;
      transition: 0.3s;
      background-color: #fff !important;
    
    }
    ul li a::after{
      content: "";
      position: absolute;
     top: 0px;
     right: 0;
      width: 0px;
      height: 4px;
      transition: 0.5s ease;
      background-color: #fff !important;
    
    }
    
    ul li:hover a::before,ul li:hover a::after{
      content: "";
    width: 100%;
    }
    
    </style>
</head>

