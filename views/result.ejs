  <style>
 body{
    
    font-family: sans-serif !important;
      }
      body {

font-family: sans-serif !important;
}

ul li {
position: relative;
}

ul li a::before {
content: "";
position: absolute;
bottom: 0px;
left: 0;
width: 0px;
height: 4px;
transition: 0.3s;
background-color: #fff !important;

}

ul li a::after {
content: "";
position: absolute;
top: 0px;
right: 0;
width: 0px;
height: 4px;
transition: 0.5s ease;
background-color: #fff !important;

}

ul li:hover a::before,
ul li:hover a::after {
content: "";
width: 100%;
}
.card {
            border-radius: 5px;
            padding: 20px;
            background-color: #ebecf0;
            border-radius: 20px;
            box-shadow: 2px 2px 5px #bfc5db, -2px -2px 10px #fff !important;
        }
        .card:hover {
            box-shadow: inset 2px 2px 5px #babecc, inset -2px -2px 10px #fff !important;
        }
  </style>
</head>

